include ../../paper.pug

-
  var periode = [
    'October 2018',
    'September 2018',
    'August 2018',
    'July 2018',
    'June 2018',
    'April 2018',
    'March 2018',
    'Febuary 2018'
  ]

+paper($t.common.blog_archived, 'folder')
  div
    .collection
      each item in periode
        |
        |
        a.waves-effect.collection-item(href='#!')
          |
          |
          span.use-text-medium
            | #{item}
          span.secondary-content
            i.material-icons.icon.primary keyboard_arrow_right
          |
          |
        |
        |
  a.btn.waves-effect.btn-flat.block
    | #{$t.common.btn_seeall}